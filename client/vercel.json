{
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/(.*)",
        "has": [
          {
            "type": "header",
            "key": "accept",
            "value": "text/html"
          }
        ],
        "destination": "/index.html"
      }
    ]
  }
  